<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="visFeld" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox fx:id="checkbox" mnemonicParsing="false" onMouseClicked="#clicked">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <Label fx:id="labelInBox" disable="true" text="=0" translateY="-20.0" />
   </children>
</VBox>
